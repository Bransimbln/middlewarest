<!DOCTYPE html>
<html lang="en">
<head>
     <meta charset="UTF-8">
     <meta name="viewport" content="width=device-width, initial-scale=1.0">
     <title>Student List</title>
     <style>
          table{
               width: 50%;
               margin: auto;
               border-collapse: collapse;
          }
          th, td{
               border: 1px solid #ccc;
               padding: 8px;
               text-align: center;
          }
          th{
               background-color: #f4f4f4;
          }
     </style>
</head>
<body>
     <h1 style=" text-align: center;">Student List</h1>
     <table>
          <thead>
               <tr>
                    <th>ID</th>
                    <th>Name</th>
                    <th>Age</th>
                    <th>Major</th>
               </tr>
          </thead>
          <tbody>
               <% students.forEach(student => { %>
                    <tr>
                         <td><%= student.id %></td>
                         <td><%= student.nama %></td>
                         <td><%= student.age %></td>
                         <td><%= student.major %></td>
                    </tr>
                    <% }); %>
          </tbody>
     </table>
     
</body>
</html>